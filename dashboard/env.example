# BotWall Dashboard Environment Variables
# Copy this file to .env.local for local development

# Backend API URL
# For development: http://localhost:3001/api
# For production: https://your-production-api.com/api
VITE_BACKEND_URL=http://localhost:3001/api

# Optional: Override the default production URL
# VITE_PRODUCTION_API_URL=https://botwall-api.onrender.com/api

# Feature flags
VITE_ENABLE_MOCK_CREDITS=true
VITE_ENABLE_ANALYTICS=true

# App configuration
VITE_APP_NAME=BotWall Dashboard
VITE_APP_VERSION=1.0.0 